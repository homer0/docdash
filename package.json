{
  "name": "docdash",
  "version": "1.0.0",
  "description": "A fork of clenemt/docdash to temporarily use in my projects",
  "main": "publish.js",
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s ../fixtures-doc -f ../fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\""
  },
  "repository": "homer0/docdash",
  "devDependencies": {
    "jsdoc": "^3.6.4",
    "browser-sync": "^2.26.7",
    "watch-run": "^1.2.5"
  },
  "author": "Clement Moron <clement.moron@gmail.com>",
  "contributors": [
    "Leonardo Apiwan (@homer0) <me@homer0.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template"
  ],
  "files": [
    "publish.js",
    "static",
    "tmpl"
  ]
}
